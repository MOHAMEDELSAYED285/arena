{
    "version": 2,
    "builds": [
      {
        "src": "backend/config/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "backend/config/wsgi.py"
      },
      {
        "src": "/admin/(.*)",
        "dest": "backend/config/wsgi.py"
      },
      {
        "src": "/api/(.*)",
        "dest": "backend/config/wsgi.py"
      }
    ]
  }